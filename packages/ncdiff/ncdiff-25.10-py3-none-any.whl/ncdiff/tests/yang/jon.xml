<?xml version='1.0' encoding='UTF-8'?>
<jon type="module" prefix="jon">
  <namespace prefix="jon">urn:jon</namespace>
  <jon:foo xmlns:jon="urn:jon" access="read-write" type="leaf" datatype="string"/>
  <jon:tracking xmlns:jon="urn:jon" access="read-write" type="container">
    <jon:enabled access="read-write" type="leaf" status="deprecated" default="false" datatype="boolean" values="true|false"/>
    <jon:enabled-v2 access="read-write" type="leaf" datatype="boolean" values="true|false"/>
    <jon:logging access="read-write" type="container">
      <jon:local access="read-write" type="leaf" datatype="boolean" values="true|false"/>
      <jon:server access="read-write" type="container">
        <jon:host access="read-write" type="leaf" datatype="string"/>
        <jon:port access="read-write" type="leaf" datatype="uint16"/>
      </jon:server>
    </jon:logging>
  </jon:tracking>
  <jon:address xmlns:jon="urn:jon" access="read-write" type="list" key="first last" ordered-by="user">
    <jon:last access="read-write" type="leaf" datatype="string" mandatory="true" is_key="true"/>
    <jon:first access="read-write" type="leaf" datatype="string" mandatory="true" is_key="true"/>
    <jon:street access="read-write" type="leaf" datatype="string"/>
    <jon:city access="read-write" type="leaf" status="deprecated" default="Unknown" datatype="string"/>
    <jon:city-v2 access="read-write" type="leaf" datatype="string"/>
  </jon:address>
  <jon:store xmlns:jon="urn:jon" access="read-write" type="leaf-list" datatype="string" ordered-by="user"/>
  <jon:location xmlns:jon="urn:jon" access="read-write" type="container">
    <jon:city access="read-write" type="choice" default="alberta" values="ontario|alberta">
      <jon:ontario access="read-write" type="case">
        <jon:ontario access="read-write" type="list" key="name">
          <jon:name access="read-write" type="leaf" datatype="string" mandatory="true" is_key="true"/>
        </jon:ontario>
      </jon:ontario>
      <jon:alberta access="read-write" type="case">
        <jon:alberta access="read-write" type="list" key="name">
          <jon:name access="read-write" type="leaf" datatype="string" mandatory="true" is_key="true"/>
        </jon:alberta>
        <jon:other-info access="read-write" type="container">
          <jon:detail access="read-write" type="leaf" datatype="string"/>
          <jon:geo-facts access="read-write" type="container">
            <jon:area access="read-write" type="leaf" datatype="int16"/>
            <jon:population access="read-write" type="leaf" datatype="int16"/>
            <jon:code access="read-write" type="leaf" default="AB" datatype="string"/>
          </jon:geo-facts>
        </jon:other-info>
      </jon:alberta>
    </jon:city>
  </jon:location>
  <jon:numbers xmlns:jon="urn:jon" access="read-write" type="container">
    <jon:number access="read-write" type="choice" values="one|two">
      <jon:one access="read-write" type="case">
        <jon:first access="read-write" type="leaf" datatype="string"/>
      </jon:one>
      <jon:two access="read-write" type="case">
        <jon:second access="read-write" type="leaf" datatype="string"/>
        <jon:third access="read-write" type="leaf" datatype="string"/>
      </jon:two>
    </jon:number>
  </jon:numbers>
  <jon:errdisable xmlns:jon="urn:jon" access="read-write" type="container">
    <jon:detect access="read-write" type="container">
      <jon:cause-config access="read-write" type="container">
        <jon:sfp-config-mismatch access="read-write" type="leaf" default="true" datatype="boolean" values="true|false"/>
        <jon:small-frame access="read-write" type="leaf" default="true" datatype="boolean" values="true|false"/>
        <jon:loopdetect access="read-write" type="leaf" default="true" datatype="boolean" values="true|false"/>
        <jon:leaflist1 access="read-write" type="leaf-list" default="abc,def,ghi" datatype="string" ordered-by="user"/>
        <jon:leaflist2 access="read-write" type="leaf-list" default="abc,def,ghi" datatype="string"/>
      </jon:cause-config>
    </jon:detect>
  </jon:errdisable>
</jon>
