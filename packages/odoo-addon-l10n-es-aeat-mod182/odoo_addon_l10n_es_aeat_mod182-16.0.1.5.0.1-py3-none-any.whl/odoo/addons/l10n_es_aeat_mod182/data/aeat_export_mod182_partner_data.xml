<odoo>
    <!-- Tipo registro 2 – Registro de declarado: -->
    <record id="aeat_mod182_partner_export_config" model="aeat.model.export.config">
        <field
            name="name"
        >Exportación modelo 182 - Tipo de Registro 2 – Registro de declarado</field>
        <field name="model_number">182</field>
    </record>
    <!--          Posiciones  Naturaleza    Descripción-->
    <!--          1           Numérico      Tipo de Registro -->
    <record
        id="aeat_mod182_partner_export_line_01"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">1</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Constante: 2</field>
        <field name="fixed_value">2</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!--          2-4     Numérico    Modelo Declaración -->
    <record
        id="aeat_mod182_partner_export_line_02"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">2</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Constante: 182</field>
        <field name="fixed_value">182</field>
        <field name="export_type">string</field>
        <field name="size">3</field>
        <field name="alignment">left</field>
    </record>
    <!--          5-8     Numérico    Ejercicio -->
    <record
        id="aeat_mod182_partner_export_line_03"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">3</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Ejercicio</field>
        <field name="expression">${object.report_id.year}</field>
        <field name="export_type">string</field>
        <field name="size">4</field>
        <field name="alignment">left</field>
    </record>
    <!--          9-17     Alfanumérico    NIF del declarante -->
    <record
        id="aeat_mod182_partner_export_line_04"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">4</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">NIF del declarante</field>
        <field name="expression">${object.report_id.company_vat}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">left</field>
    </record>
    <!--          18-26     Alfanumérico    NIF del declarado -->
    <record
        id="aeat_mod182_partner_export_line_05"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">5</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">NIF del declarado</field>
        <field name="expression">${object.partner_vat}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">left</field>
    </record>
    <!--          27-35     Alfanumérico    NIF del representante legal -->
    <record
        id="aeat_mod182_partner_export_line_06"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">6</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">NIF del representate legal</field>
        <field name="expression">${object.representative_vat}</field>
        <field name="export_type">string</field>
        <field name="size">9</field>
        <field name="alignment">left</field>
    </record>
    <!--          36-75     Numérico    Apellidos y nombre o razón social del declarado -->
    <record
        id="aeat_mod182_partner_export_line_07"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">7</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Apellidos y nombre o razón social del declarado</field>
        <field name="expression">${object.partner_id.name}</field>
        <field name="export_type">string</field>
        <field name="size">40</field>
        <field name="alignment">left</field>
    </record>
    <!--          76-77     Numérico    Código provincia -->
    <record
        id="aeat_mod182_partner_export_line_08"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">8</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Código provincia</field>
        <field name="expression">${object.partner_state_code or '99'}</field>
        <field name="export_type">string</field>
        <field name="size">2</field>
        <field name="alignment">left</field>
    </record>
    <!--          78     Alfabético    Clave -->
    <record
        id="aeat_mod182_partner_export_line_09"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">9</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Clave</field>
        <field name="expression">${object.operation_key}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!--          79-83     Numérico    % de deducción -->
    <record
        id="aeat_mod182_partner_export_line_10"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">10</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">% de deducción</field>
        <field name="expression">${object.deduction_percentage}</field>
        <field name="export_type">float</field>
        <field name="size">5</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          84-96     Numérico    Importe o valoración del donativo, -->
    <!--                                aportación o disposición -->
    <record id="aeat_mod349_main_export_line_11" model="aeat.model.export.config.line">
        <field name="sequence">11</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field
            name="name"
        >Importe o valoración del donativo, aportación o disposición</field>
        <field name="expression">${abs(object.donation_amount)}</field>
        <field name="size">13</field>
        <field name="decimal_size">2</field>
        <field name="alignment">left</field>
    </record>
    <!--          97     Alfabético    Donativo, aportación o disposición en especie -->
    <record id="aeat_mod349_main_export_line_13" model="aeat.model.export.config.line">
        <field name="sequence">13</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Donativo, aportación o disposición en especie</field>
        <field name="expression">${object.donation_or_contribution_in_kind}</field>
        <field name="export_type">boolean</field>
        <field name="size">1</field>
        <field name="bool_yes">X</field>
        <field name="bool_no"> </field>
        <field name="alignment">left</field>
    </record>
    <!--          98-99     Numérico    Deducción com. autónoma -->
    <record
        id="aeat_mod182_partner_export_line_14"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">14</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Deducción com. autónoma</field>
        <field name="expression">${object.regional_community_deduction_id}</field>
        <field name="export_type">string</field>
        <field name="apply_sign" eval="False" />
        <field name="size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          100-104     Numérico    % de deducción com. autónoma -->
    <record
        id="aeat_mod182_partner_export_line_15"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">15</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">% de deducción com. autónoma</field>
        <field name="expression">${object.regional_deduction_percentage}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True" />
        <field name="positive_sign"> </field>
        <field name="negative_sign">N</field>
        <field name="size">5</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          105     Alfabético    Naturaleza de declarado -->
    <record
        id="aeat_mod182_partner_export_line_16"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">16</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Naturaleza de declarado</field>
        <field name="expression">${object.declared_nature}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">left</field>
    </record>
    <!--          106     Alfabético    Revocación -->
    <record
        id="aeat_mod182_partner_export_line_17"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">17</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Revocación</field>
        <field name="expression">${object.revocation}</field>
        <field name="export_type">boolean</field>
        <field name="size">1</field>
        <field name="bool_yes">X</field>
        <field name="bool_no"> </field>
        <field name="alignment">left</field>
    </record>
    <!--          107-110     Numérico    Ejercicio en que se efectuó la donación -->
    <!--                                  revocada -->
    <record
        id="aeat_mod182_partner_export_line_18"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">18</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Ejercicio en que se efectuó la donación revocada</field>
        <field name="expression">${object.year_of_revoked_donation}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True" />
        <field name="positive_sign"> </field>
        <field name="negative_sign">N</field>
        <field name="size">4</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          111     Alfabético    Tipo de bien -->
    <record
        id="aeat_mod182_partner_export_line_19"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">19</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Tipo de bien</field>
        <field name="expression">${object.kind_of_good}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">right</field>
    </record>
    <!--          112-131     Alfanumérico    Identificación del bien -->
    <record
        id="aeat_mod182_partner_export_line_20"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">20</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Identificación del bien</field>
        <field name="expression">${object.item_identification}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True" />
        <field name="positive_sign"> </field>
        <field name="negative_sign">N</field>
        <field name="size">20</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          132     Numérico    Recurrencia donativos -->
    <record
        id="aeat_mod182_partner_export_line_21"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">21</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Recurrencia donativos</field>
        <field name="expression">${object.donation_recurrence}</field>
        <field name="export_type">string</field>
        <field name="size">1</field>
        <field name="alignment">right</field>
    </record>
    <!--          133-141     Alfanumérico    NIF del titular del patrimonio protegido -->
    <record
        id="aeat_mod182_partner_export_line_22"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">22</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">NIF del titular del patrimonio protegido</field>
        <field name="expression">${object.protected_patrimony_holder_nif}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True" />
        <field name="positive_sign"> </field>
        <field name="negative_sign">N</field>
        <field name="size">9</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          142-181     Alfabético    Apellidos y nombre del titular -->
    <!--                                    del patrimonio protegido -->
    <record
        id="aeat_mod182_partner_export_line_23"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">23</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field
            name="name"
        >Apellidos y nombre del titular del patrimonio protegido</field>
        <field name="expression">${object.protected_patrimony_holder_name}</field>
        <field name="export_type">float</field>
        <field name="apply_sign" eval="True" />
        <field name="positive_sign"> </field>
        <field name="negative_sign">N</field>
        <field name="size">40</field>
        <field name="decimal_size">2</field>
        <field name="alignment">right</field>
    </record>
    <!--          182-250     Alfabético    Blancos -->
    <record
        id="aeat_mod182_partner_export_line_24"
        model="aeat.model.export.config.line"
    >
        <field name="sequence">24</field>
        <field name="export_config_id" ref="aeat_mod182_partner_export_config" />
        <field name="name">Blancos</field>
        <field name="fixed_value" />
        <field name="export_type">string</field>
        <field name="size">69</field>
        <field name="alignment">left</field>
    </record>
</odoo>
