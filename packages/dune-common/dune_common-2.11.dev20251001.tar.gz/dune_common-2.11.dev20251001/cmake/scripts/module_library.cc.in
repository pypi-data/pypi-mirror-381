#include <string>

std::size_t ${module_lib_mangled}_version()
{
  return ${ProjectVersionMajor} * 10000 + ${ProjectVersionMinor} * 100 + ${ProjectVersionRevision};
}

std::string ${module_lib_mangled}_version_string()
{
  return "${ProjectVersion}";
}
