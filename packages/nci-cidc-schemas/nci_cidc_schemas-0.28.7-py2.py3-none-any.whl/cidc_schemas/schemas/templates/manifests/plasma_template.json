                            "type_ref": "sample.json#properties/sample_replacement",
                            "allow_empty": true
                        },
                        "residual sample use": {
                            "merge_pointer": "/residual_sample_use",
                            "type_ref": "sample.json#properties/residual_sample_use",
                            "allow_empty": true
                        },
                        "comments": {
                            "merge_pointer": "/comments",
                            "type_ref": "sample.json#properties/comments",
                            "allow_empty": true
                        }
                    }
                }
            }
        }
    }
}
