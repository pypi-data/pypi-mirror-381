        "@babel/register": "^7.16.0",
        "@babel/runtime-corejs3": "7.16.8",
        "cmd-ts": "^0.13.0",
        "js-yaml": "^4.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.5.5"
    },
    "devDependencies": {
        "@exabyte-io/eslint-config": "^2022.11.17-0",
        "@exabyte-io/wode.js": "git+https://github.com/Exabyte-io/wode.js.git#9798f9769020c79830730b26acb1e22ac7914a28",
