<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.example.com/deepwrap"
           xmlns:tns="http://www.example.com/deepwrap"
           elementFormDefault="qualified">

    <xs:complexType name="LeafType">
        <xs:sequence>
            <xs:element name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="InnerWrapperType">
        <xs:sequence>
            <xs:element name="Leaf" type="tns:LeafType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="WrapperType">
        <xs:sequence>
            <xs:element name="Inner" type="tns:InnerWrapperType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ContainerType">
        <xs:sequence>
            <xs:element name="Wrapper" type="tns:WrapperType"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:element name="Container" type="tns:ContainerType"/>

</xs:schema>
