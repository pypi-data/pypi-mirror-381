<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://example.com/entries" xmlns:tns="http://example.com/entries" elementFormDefault="qualified">

  <xs:complexType name="ChildEntryType">
    <xs:sequence>
      <xs:element name="value" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="WrapperType">
    <xs:sequence>
      <!-- Repeating entry-like child -->
      <xs:element name="Entry" type="tns:ChildEntryType" maxOccurs="unbounded"/>
      <!-- Other optional entry-like members that would typically become JSON placeholders -->
      <xs:element name="FooEntry" type="tns:ChildEntryType" minOccurs="0"/>
      <xs:element name="BarEntry" type="tns:ChildEntryType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="RootType">
    <xs:sequence>
      <xs:element name="Wrapper" type="tns:WrapperType"/>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
