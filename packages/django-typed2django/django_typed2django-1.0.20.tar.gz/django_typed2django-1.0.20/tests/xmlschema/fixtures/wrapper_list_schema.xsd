<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.example.com/wrapperlist"
           xmlns:tns="http://www.example.com/wrapperlist"
           elementFormDefault="qualified">

    <!-- Wrapper contains a list wrapper with many Sample leafs -->
    <xs:complexType name="SampleType">
        <xs:sequence>
            <xs:element name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SamplesListType">
        <xs:sequence>
            <xs:element name="Sample" type="tns:SampleType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ComponentStreamType">
        <xs:sequence>
            <xs:element name="SamplesList" type="tns:SamplesListType" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="componentId" type="xs:string" use="required"/>
    </xs:complexType>

    <xs:complexType name="DeviceStreamType">
        <xs:sequence>
            <xs:element name="ComponentStream" type="tns:ComponentStreamType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="DeviceStream" type="tns:DeviceStreamType"/>

</xs:schema>
