{
  "name": "@amzn/amazon_sagemaker_sql_editor",
  "version": "0.1.19",
  "description": "Jupyter extension for SQL editing capabilities within Amazon SageMaker notebooks",
  "private": true,
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://aws.amazon.com/sagemaker/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Amazon"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "workspaces": [
    "ui-tests"
  ],
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "amazon_sagemaker_sql_editor"
        }
      }
    },
    "schemaDir": "schema",
    "outputDir": "amazon_sagemaker_sql_editor/labextension",
    "sharedPackages": {
      "@jupyter-lsp/jupyterlab-lsp": {
        "bundled": false,
        "singleton": true
      },
      "@lezer/common": {
        "bundled": true,
        "singleton": true,
        "requiredVersion": "^1.2.0"
      },
      "@mui/system": {
        "requiredVersion": "^5.14.12"
      },
      "@emotion/react": {
        "requiredVersion": "^11.11.2"
      }
    },
    "disabledExtensions": [
      "@jupyter-lsp/jupyterlab-lsp:syntax_highlighting",
      "@jupyter-lsp/jupyterlab-lsp:ipython-sql",
      "@jupyter-lsp/jupyterlab-lsp:code-jumpers",
      "@jupyter-lsp/jupyterlab-lsp:hover"
    ]
  },
  "scripts": {
    "copy:sql-language-server": "rimraf amazon_sagemaker_sql_editor/sql-language-server && mkdir amazon_sagemaker_sql_editor/sql-language-server && cp ./npm-overrides/package.json amazon_sagemaker_sql_editor/sql-language-server && cd amazon_sagemaker_sql_editor/sql-language-server && npm install --prefix ./ sql-language-server vscode-jsonrpc",
    "build": "npm run build:lib && npm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "build:prod": "npm run copy:sql-language-server && npm run clean && npm run lint && npm run build:lib:prod && npm run build:labextension",
    "release": "npm run build:prod",
    "clean": "rimraf lib tsconfig.tsbuildinfo && rimraf amazon_sagemaker_sql_editor/labextension amazon_sagemaker_sql_editor/_version.py && rimraf .eslintcache .stylelintcache",
    "eslint": "npm run eslint:check --fix",
    "eslint:check": "eslint src --cache --ext .ts,.tsx",
    "install:extension": "npm run build",
    "lint": "npm run stylelint && npm run prettier && npm run eslint",
    "lint:check": "npm run stylelint:check && npm run prettier:check && npm run eslint:check",
    "prettier": "npm run prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\" --write",
    "prettier:check": "npm run prettier:base --check",
    "stylelint": "npm run stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@codemirror/lang-sql": "^6.0.0",
    "@codemirror/language": "^6.0.0",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.2",
    "@emotion/styled": "^11.11.0",
    "@jupyter-lsp/jupyterlab-lsp": "^5.0.1",
    "@jupyterlab/application": "^4.0.10",
    "@jupyterlab/apputils": "^4.1.10",
    "@jupyterlab/cells": "^4.0.10",
    "@jupyterlab/codeeditor": "^4.0.10",
    "@jupyterlab/codemirror": "^4.0.10",
    "@jupyterlab/fileeditor": "^4.0.10",
    "@jupyterlab/lsp": "^4.0.10",
    "@jupyterlab/notebook": "^4.0.10",
    "@jupyterlab/tooltip": "^4.0.10",
    "@jupyterlab/ui-components": "^4.0.10",
    "@lezer/common": "^1.2.0",
    "@lumino/widgets": "^2.0.1",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.14.9",
    "@mui/system": "^5.14.12",
    "react": "^18.2.0",
    "react-arborist": "^3.4.0",
    "react-dom": "^18.2.0",
    "sql-formatter": "^12.2.1",
    "use-resize-observer": "^9.1.0",
    "vscode-jsonrpc": "^8.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.10",
    "@jupyterlab/completer": "^4.0.10",
    "@jupyterlab/logconsole": "^4.0.10",
    "@jupyterlab/testutils": "^4.0.10",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^13.0.0",
    "@types/codemirror": "^5.60.15",
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^20.3.3",
    "@types/react": "^18.0.26",
    "@types/react-addons-linked-state-mixin": "^0.14.22",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.0.2",
    "yjs": "^13.5.40"
  },
  "overrides": {
    "@jupyterlab/completer": "^4.0.10",
    "stylelint": {
      "micromatch": "^4.0.8"
    },
    "form-data": "^4.0.4"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts",
    "tests",
    "**/__tests__",
    "ui-tests"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "interface",
          "format": [
            "PascalCase"
          ],
          "custom": {
            "regex": "^I[A-Z]",
            "match": true
          }
        }
      ],
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": false
        }
      ],
      "curly": [
        "error",
        "all"
      ],
      "eqeqeq": "error",
      "prefer-arrow-callback": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "printWidth": 100,
    "tabWidth": 2
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "plugins": [
      "stylelint-csstree-validator"
    ],
    "rules": {
      "csstree/validator": true,
      "property-no-vendor-prefix": null,
      "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
