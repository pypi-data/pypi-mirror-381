[build-system]
requires = ['setuptools>=61.0']
build-backend = 'setuptools.build_meta'

[project]
name = '{{ project.name.snake_case }}'
version = '0.0.1'
authors = [
  { name='{{ project.author.name }}', email='{{ project.author.email }}' },
]
description = '{{ project.description }}'
readme = 'README.md'
requires-python = '>=3.7'
dependencies = [
    'uwsgi == 2.0.27',
    'passlib[bcrypt]',
    'bcrypt==4.3.0',
    'python-jose[cryptography]'
]