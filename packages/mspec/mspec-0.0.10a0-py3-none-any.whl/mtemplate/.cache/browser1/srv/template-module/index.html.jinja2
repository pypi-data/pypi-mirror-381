<!DOCTYPE html>
<html>

    <head>
        <title>{{ project.name.snake_case }}</title>
        <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
        <meta http-equiv="Pragma" content="no-cache" />
        <meta http-equiv="Expires" content="0" />
        <link rel="stylesheet" type="text/css" href="/style.css">
        <script src="./index.js"></script>
    </head>

    <body>
        <h1>{{ module.name.lower_case }}</h1>
        <p>
            <a href="/">{{ project.name.snake_case }}</a> . <a href="/{{ module.name.kebab_case }}">{{ module.name.lower_case }}</a>
        </p>

        <h2>sub sections</h2>
        <ul>
            {% for model in module.models.values() %}
            <li><a href="/{{ module.name.kebab_case }}/{{ model.name.kebab_case }}">{{ model.name.lower_case }}</a></li>
            {% endfor %}
        </ul>
    </body>

</html>