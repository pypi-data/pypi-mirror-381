<odoo>
<record id="faktur_pajak_keluaran_ou_group" model="res.groups">
    <field name="name">Operating Unit</field>
    <field
            name="category_id"
            ref="ssi_l10n_id_taxform_faktur_pajak.faktur_pajak_keluaran_data_ownership_module_category"
        />
</record>

<record
        id="ssi_l10n_id_taxform_faktur_pajak.faktur_pajak_keluaran_company_group"
        model="res.groups"
    >
    <field name="name">Company</field>
    <field
            name="category_id"
            ref="ssi_l10n_id_taxform_faktur_pajak.faktur_pajak_keluaran_data_ownership_module_category"
        />
    <field name="implied_ids" eval="[(4, ref('faktur_pajak_keluaran_ou_group'))]" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>
</odoo>
