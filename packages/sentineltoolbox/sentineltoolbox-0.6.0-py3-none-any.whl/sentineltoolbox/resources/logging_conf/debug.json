{
  "version": 1,
  "disable_existing_loggers": false,
  "filters": {
    "skip_eopf_logger": {
      "()": "sentineltoolbox.logging_utils.EopfFilter"
    }
  },
  "formatters": {
    "default": {
      "format": "[%(levelname)-8s][%(name)s] %(asctime)s  %(module)s.%(funcName)s:%(lineno)d: %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "DEBUG",
      "stream": "ext://sys.stderr",
      "filters": [
        "skip_eopf_logger"
      ]
    }
  },
  "root": {
    "handlers": [
      "console"
    ],
    "level": "DEBUG"
  },
  "loggers": {
    "eopf.store.safe": {
      "level": "INFO"
    },
    "eopf.accessor.xml_accessor": {
      "level": "INFO"
    }
  }
}
