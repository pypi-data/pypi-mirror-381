{
  "version": 1,
  "disable_existing_loggers": false,
  "filters": {
    "skip_eopf_logger": {
      "()": "sentineltoolbox.logging_utils.EopfFilter"
    }
  },
  "formatters": {
    "default": {
      "class": "sentineltoolbox.logging_utils.ColoredFormatter",
      "format": "%(log_color)s[%(levelname)-8s]%(reset)s[%(name)s]%(asctime)s %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "level": "INFO",
      "stream": "ext://sys.stderr",
      "filters": [
        "skip_eopf_logger"
      ]
    }
  },
  "root": {
    "handlers": [
      "console"
    ],
    "level": "INFO"
  },
  "loggers": {
    "eopf.store.safe": {
      "level": "INFO"
    },
    "eopf.accessor.xml_accessor": {
      "level": "INFO"
    },
    "rasterio._env": {
      "level": "CRITICAL"
    },
    "sentineltoolbox": {
      "level": "INFO"
    }
  }
}
