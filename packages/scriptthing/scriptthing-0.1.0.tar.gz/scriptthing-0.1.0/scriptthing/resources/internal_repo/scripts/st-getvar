#!/usr/bin/env scriptthing-run
# lang: py
from scriptthing.utils import store
import sys
if __name__ == '__main__':
    key = sys.argv[1] if len(sys.argv) > 1 else ''
    if not key:
        print('')
    else:
        try:
            print(store.get(key, ''))
        except Exception:
            print('')