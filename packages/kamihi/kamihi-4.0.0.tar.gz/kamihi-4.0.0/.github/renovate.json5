{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  labels: [
    "type:task"
  ],
  reviewers: [
    "kiinami"
  ],
  ignorePaths: [
    "requirements.txt"
  ],
  lockFileMaintenance: {
    enabled: true,
    automerge: true
  },
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
}
